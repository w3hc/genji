{
  "trailingComma": "es5",
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "bracketSameLine": true,
  "useTabs": false,
  "tabWidth": 2,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
